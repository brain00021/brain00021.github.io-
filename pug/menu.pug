  // menu
  #menu.menu-position
    // Brand and toggle get grouped for better mobile display
    .container
      .navbar-header
        button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#bs-example-navbar-collapse-1', aria-expanded='false')
          span.sr-only Toggle navigation
          span.icon-bar
          span.icon-bar
          span.icon-bar
        a.navbar-brand(href='#')
          img(src='images/logo@1x.png', alt='logo')
      #bs-example-navbar-collapse-1.collapse.navbar-collapse
        ul.nav.navbar-nav
          .row
            li.col-md-3.col-xs-12(data-menuanchor='home')
              a(href='#home')
                p Home
            li.col-md-3.col-xs-12(data-menuanchor='recent-projecte')
              a(href='#recent-project')
                p Recent Project
            li.col-md-3.col-xs-12(data-menuanchor='resume')
              a(href='#resume')
                p Resume
            li.col-md-3.col-xs-12(data-menuanchor='contect-me')
